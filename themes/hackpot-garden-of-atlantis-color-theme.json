{
    "name": "Hackpot Garden Of Atlantis",
    "colors": {
        "activityBarBadge.background": "#1e621e",
        "activityBar.activeBorder": "#9F6934",
        "activityBar.activeBackground": "#002828",
        "activityBar.background": "#002020",
        "activityBar.foreground": "#32CD32",
        "activityBar.inactiveForeground": "#32CD3295",
        "badge.background": "#002828",
        "badge.foreground": "#b1e7b1",
        "breadcrumb.background": "#002828",
        "breadcrumb.foreground": "#1e621e",
        "button.background": "#1e621e",
        "button.foreground": "#b1e7b1",
        "dropdown.background": "#002020",
        "dropdown.border": "#1e621e",
        "editorBracketMatch.border": "#1e621e",
        "editorCursor.foreground": "#32CD32",
        "editorGroupHeader.tabsBackground": "#002020",
        "editorGroup.border": "#002828",
        "editorHoverWidget.background": "#002828",
        "editorHoverWidget.border": "#1e621e",
        "editorHoverWidget.foreground": "#b1e7b1",
        "editorIndentGuide.background": "#002828",
        "editorLineNumber.activeForeground": "#32CD32",
        "editorLineNumber.foreground": "#1e621e",
        "editorMarkerNavigationError.background": "#ff661a",
        "editorMarkerNavigationWarning.background": "#002828",
        "editorPane.background": "#002020",
        "editorSuggestWidget.selectedBackground": "#003030",
        "editorUnnecessaryCode.opacity": "#000000c0",
        "editorWidget.background": "#002828",
        "editorWidget.border": "#1e621e",
        "editor.background": "#002020",
        "editor.findMatchHighlightBackground": "#003030",
        "editor.foldBackground": "#00303050",
        "editor.foreground": "#b1e7b1",
        "editor.hoverHighlightBackground": "#002828",
        "editor.lineHighlightBackground": "#00303050",
        "editor.rangeHighlightBackground": "#002020",
        "editor.selectionBackground": "#003030",
        "errorForeground": "#b1e7b1",
        "focusBorder": "#1e621e",
        "inputOption.activeBorder": "#9F6934",
        "inputValidation.errorBackground": "#002828",
        "inputValidation.errorBorder": "#1e621e",
        "inputValidation.infoBackground": "#002828",
        "inputValidation.infoBorder": "#1e621e",
        "inputValidation.warningBackground": "#002828",
        "inputValidation.warningBorder": "#1e621e",
        "input.background": "#002020",
        "input.foreground": "#b1e7b1",
        "input.placeholderForeground": "#b1e7b1",
        "list.activeSelectionBackground": "#00303050",
        "list.activeSelectionForeground": "#32CD32",
        "list.dropBackground": "#002828",
        "list.focusBackground": "#00303050",
        "list.highlightForeground": "#32CD32",
        "list.hoverBackground": "#00303050",
        "list.inactiveSelectionBackground": "#002020",
        "list.inactiveSelectionForeground": "#32CD32",
        "minimap.findMatchHighlight": "#b1e7b1",
        "minimap.selectionHighlight": "#32CD32",
        "notificationCenterHeader.foreground": "#32CD32",
        "notificationCenterHeader.background": "#003030",
        "notifications.background": "#002828",
        "notifications.foreground": "#b1e7b1",
        "panelTitle.activeBorder": "#9F6934",
        "panelTitle.activeForeground": "#32CD32",
        "panel.border": "#1e621e",
        "peekViewTitle.background": "#002828",
        "peekView.border": "#1e621e",
        "peekViewResult.background": "#002828",
        "peekViewEditor.background": "#002020",
        "peekViewEditorGutter.background": "#002828",
        "peekViewEditor.matchHighlightBackground": "#1e621e",
        "pickerGroup.border": "#1e621e",
        "pickerGroup.foreground": "#002828",
        "progressBar.background": "#1e621e",
        "selection.background": "#002828",
        "scrollbarSlider.activeBackground": "#003838",
        "scrollbarSlider.background": "#003030",
        "scrollbarSlider.hoverBackground": "#003838",
        "settings.checkboxBorder": "#1e621e",
        "settings.checkboxForeground": "#b1e7b1",
        "settings.dropdownForeground": "#b1e7b1",
        "settings.headerForeground": "#32CD32",
        "settings.modifiedItemIndicator": "#9F6934",
        "settings.numberInputBackground": "#002828",
        "settings.textInputBackground": "#002828",
        "settings.textInputForeground": "#b1e7b1",
        "sideBarSectionHeader.background": "#002828",
        "sideBarSectionHeader.foreground": "#9F6934",
        "sideBarTitle.foreground": "#32CD32",
        "sideBar.background": "#002020",
        "sideBar.border": "#1e621e",
        "sideBar.foreground": "#b1e7b1",
        "statusBar.background": "#002020",
        "statusBar.foreground": "#32CD32",
        "statusBar.noFolderBackground": "#002020",
		"statusBar.noFolderForeground": "#32CD32",
        "tab.activeBackground": "#002828",
        "tab.activeBorder": "#9F6934",
        "tab.activeForeground": "#32CD32",
        "tab.border": "#1e621e",
        "tab.inactiveBackground": "#002020",
        "tab.inactiveForeground": "#32CD3290",
        "tab.unfocusedActiveForeground": "#32CD3295",
        "tab.unfocusedActiveBorder": "#9F693495",
        "tab.unfocusedInactiveForeground": "#32CD3290",
        "terminal.ansiBlack": "#002828",
        "terminal.ansiRed": "#ff704d",
        "terminal.ansiGreen": "#1e621e",
        "terminal.ansiYellow": "#ffff99",
        "terminal.ansiBlue": "#268BD2",
        "terminal.ansiMagenta": "#9933ff",
        "terminal.ansiCyan": "#1b9891",
        "terminal.ansiWhite": "#b1e7b1",
        "terminal.ansiBrightBlack": "#003030",
        "terminal.ansiBrightRed": "#ff661a",
        "terminal.ansiBrightGreen": "#32CD32",
        "terminal.ansiBrightYellow": "#ffff1a",
        "terminal.ansiBrightBlue": "#1fada6",
        "terminal.ansiBrightMagenta": "#b366ff",
        "terminal.ansiBrightCyan": "#00ffff",
        "terminal.ansiBrightWhite": "#c4edc4",
        "terminal.foreground": "#b1e7b1",
        "tree.indentGuidesStroke": "#002828"
    },
    "tokenColors": [
        {
            "name": "Dark Green",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "markup.quote.markdown"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#1e621e"
            }
        },
        {
            "name": "Red Orange",
            "scope": [
                "markup.deleted",
                "support.type.exception",
                "token.error-token"
            ],
            "settings": {
                "foreground": "#ff661a"
            }
        },
        {
            "name": "Light Red",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "foreground": "#ff704d"
            }
        },
        {
            "name": "Bright Green",
            "scope": [
                "entity.name.function",
                "entity.name.tag",
                "meta.function-call",
                "entity.name.type",
                "meta.embedded.block.html",
                "variable.other.global",
                "string.other.link.description.markdown",
                "variable.other.property",
                "keyword.other.unit",
                "markup.list",
                "string.other.link.title"
            ],
            "settings": {
                "foreground": "#32CD32"
            }
        },
        {
            "name": "Tan",
            "scope": [
                "storage.type"
            ],
            "settings": {
                "foreground": "#d0a97c"
            }
        },
        {
            "name": "Sea Green",
            "scope": [
                "support"
            ],
            "settings": {
                "foreground": "#00b359"
            }
        },
        {
            "name": "Blue",
            "scope": [
                "meta.function.url",
                "markup.underline.link",
                "meta.link.reference",
                "variable.parameter.url"
            ],
            "settings": {
                "foreground": "#268BD2"
            }
        },
        {
            "name": "Purple",
            "scope": [
                "constant.language",
                "entity.name.section.markdown",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#b366ff"
            }
        },
        {
            "name": "Pink",
            "scope": [
                "constant.numeric",
                "constant.character.numeric"
            ],
            "settings": {
                "foreground": "#e56697"
            }
        },
        {
            "name": "Aqua Blue",
            "scope": [
                "string.quoted",
                "punctuation.definition.string",
                "markup.fenced_code.block.markdown",
                "string.interpolated",
                "string.template",
                "markup.inline.raw.string"
            ],
            "settings": {
                "foreground": "#1fada6"
            }
        },
        {
            "name": "Yellow",
            "scope": [
                "constant.character.escape",
                "constant.other.placeholder",
                "constant.other.character-class",
                "constant.character.entity",
                "keyword.operator.new",
                "keyword.operator.ternary",
                "keyword.operator.quantifier",
                "keyword.operator.negation",
                "keyword.operator.unpacking",
                "keyword.operator.arithmetic",
                "keyword.operator.increment",
                "keyword.operator.decrement",
                "keyword.operator.channel",
                "keyword.operator.redirect",
                "keyword.operator.expansion",
                "keyword.operator.spread",
                "keyword.operator.rest",
                "keyword.operator.address",
                "keyword.operator.ellipsis",
                "punctuation.vararg-ellipses",
                "meta.separator.markdown",
                "markup.inserted.math.display.markdown"
            ],
            "settings": {
                "foreground": "#ffff4d"
            }
        },
        {
            "name": "Brown",
            "scope": [
                "keyword",
                "storage.type.js",
                "storage.type.function",
                "entity.other",
                "support.type.property-name",
                "entity.name.tag.localname",
                "entity.other.attribute-name.html",
                "markup.raw.block.markdown"
            ],
            "settings": {
                "foreground": "#9F6934"
            }
        },
        {
            "name": "Orange",
            "scope": [
                "punctuation",
                "keyword.operator",
                "meta.brace",
                "constant.character",
                "markup.heading.setext",
                "punctuation.definition.list",
                "storage.type.function.arrow",
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#ffb84d"
            }
        },
        {
            "name": "Light Green",
            "scope": [
                "meta.embedded.block",
                "meta.assertion",
                "meta.group.assertion",
                "meta.function-call.arguments",
                "variable.other",
                "variable.other.property",
                "entity.other.attribute-name",
                "string.unquoted.html",
                "meta.object",
                "constant.other",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#b1e7b1"
            }
        },
        {
            "name": "Mint",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#76d576"
            }
        }
    ]
}