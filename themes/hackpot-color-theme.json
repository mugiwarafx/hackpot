	"tokenColors": [
		{
			"name": "Green",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"markup.quote.markdown"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#1e621e"
			}
		},
		{
			"name": "Red Orange",
			"scope": [
				"markup.deleted",
				"token.error-token"
			],
			"settings": {
				"foreground": "#FF4500"
			}
		},
		{
			"name": "Bright Green",
			"scope": [
				"entity.name.function",
				"entity.name.tag",
				"meta.function-call",
				"support.function",
				"storage.type",
				"entity.name.type",
				"support.variable",
				"meta.embedded.block.html",
				"variable.other.global",
				"support.constant.property-value",
				"string.other.link.description.markdown",
				"support.type.property-name.json",
				"support.class.builtin",
				"variable.other.property",
				"keyword.other.unit",
				"markup.list"
			],
			"settings": {
				"foreground": "#32CD32"
			}
		},
		{
			"name": "Blue",
			"scope": [
				"meta.function.url",
				"markup.underline.link",
				"meta.link.reference",
				"variable.parameter.url"
			],
			"settings": {
